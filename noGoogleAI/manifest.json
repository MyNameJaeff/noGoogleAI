{
  "manifest_version": 2,
  "name": "noGoogleAI",
  "version": "1.0.1",
  "description": "Remove Google AI from your search results.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.google.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "icons/robot-icon.png"
  }
}